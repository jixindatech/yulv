{
  "roles": ["admin"],
  "admin": [
    {
      "path": "/",
      "children": [{
        "path": "dashboard"
      }],
      "api": [
        "GET:/user/info",
        "POST:/system/user/logout"
      ]
    },
    {
      "path": "/db",
      "children": [
        {
          "path":"database",
          "api": [
            "GET:/api/db",
            "POST:/api/db",
            "PUT:/api/db/:id",
            "GET:/api/db/:id",
            "DELETE:/api/db/:id"
          ]
        },
        {
          "path": "dbuser",
          "api": [
            "GET:/api/dbuser",
            "POST:/api/dbuser",
            "PUT:/api/dbuser/:id",
            "GET:/api/dbuser/:id",
            "DELETE:/api/dbuser/:id",
            "PUT:/api/dbuser/:id/db",
            "POST:/api/dbuser/distribute"
          ]
        }
      ]
    },
    {
      "path": "/policy",
      "children": [
        {
          "path":"ip",
          "api": [
            "GET:/api/ip",
            "POST:/api/ip",
            "PUT:/api/ip/:id",
            "GET:/api/ip/:id",
            "DELETE:/api/ip/:id",
            "POST:/api/ip/distribute"
          ]
        },
        {
          "path": "rule",
          "api": [
            "GET:/api/rule",
            "POST:/api/rule",
            "PUT:/api/rule/:id",
            "GET:/api/rule/:id",
            "DELETE:/api/rule/:id",
            "GET:/api/rule/sql/test",
            "POST:/api/rule/distribute"
          ]
        }
      ]
    },
    {
      "path": "/system",
      "children": [
        {
          "path": "user",
          "api": [
            "GET:/system/user",
            "POST:/system/user",
            "PUT:/system/user",
            "GET:/system/user/:id",
            "PUT:/system/user/:id",
            "DELETE:/system/user/:id",
            "PUT:/system/user/password/:id"
          ]
        },
        {
          "path": "config",
          "api": [
            "GET:/system/email",
            "POST:/system/email",
            "PUT:/system/email/:id",
            "GET:/system/ldap",
            "POST:/system/ldap",
            "PUT:/system/ldap/:id"
          ]
        }]
    },
    {
      "path": "/"
    }
  ]
}